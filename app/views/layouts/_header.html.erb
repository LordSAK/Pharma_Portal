<header class="navbar navbar-fixed-top navbar-inverse header">
  <div class="navbar-inner">
    <div class="container" style="margin-left:15px;">
      <br/>
      <%= link_to "Brand 'X'", '#', id: "logo" %>
      <div style="float:right;color:#FFF;margin:5px 15px 0 0">
        <% if !current_user.blank? %>
          <% if current_user.userType == "Physician" %>
             <%= "#{current_user.suffix} #{current_user.physicianFirstName} #{current_user.physicianMiddleName} #{current_user.physicianLastName}" %>
          <% elsif current_user.userType == "Hospital" %>
              <%= @current_user.teachingHospitalName %>
          <% elsif current_user.userType == "Employee" %>
               <%= "#{current_user.FirstName} #{current_user.LastName}" %>
          <% end %>
          &nbsp;&nbsp;|&nbsp;&nbsp;<%= link_to "Sign out",signout_path, method: "delete" %>
        <%end%>
      </div>
    </div>
  </div>
</header>

